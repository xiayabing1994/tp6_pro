        }
    ],
    "require": {
        "php": "^7.1",
        "alchemy/binary-driver": "^2.0",
        "evenement/evenement": "^2.0",
        "neutron/temporary-filesystem": "2.3",
        "sabre/cache": "^1.0"
    },
    "suggest": {
        "php-ffmpeg/extras": "A compilation of common audio & video drivers for PHP-FFMpeg"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-0": {
            "FFMpeg\\": "src"
